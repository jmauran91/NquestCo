{
  "name": "react-webpack-boilerplate",
  "version": "2.1.0",
  "description": "Minimalistic ES6 React boilerplate",
  "main": "index.js",
  "scripts": {
    "start": "node --use_strict index.js",
    "start:front": "cross-env npm run clean && webpack --config webpack.config.js --progress --watch",
    "clean": "rimraf dist",
    "production": "cross-env npm run clean && webpack --config webpack.production.config.js --progress --profile --colors",
    "lint": "eslint ./app/**/**.js",
    "test": "jest",
    "test:watch": "jest --watch",
    "coverage": "jest --coverage",
    "precommit": "cross-env npm run lint && npm test",
    "prepush": "cross-env npm run lint && npm test"
  },
  "author": "Kliment Petrov <kleopetroff@gmail.com>",
  "license": "MIT",
  "jest": {
    "setupFiles": [
      "<rootDir>/config/jest/shim.js"
    ],
    "mapCoverage": true,
    "collectCoverageFrom": [
      "app/**/*.{js,jsx,ts,tsx}"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/config/jest/assetsTransformer.js",
      "\\.(css|less)$": "<rootDir>/config/jest/assetsTransformer.js"
    },
    "testMatch": [
      "**/?(*.)(spec|test).js?(x)"
    ],
    "transform": {
      "\\.js$": "babel-jest"
    }
  },
  "dependencies": {
    "async": "^2.6.0",
    "atob": "^2.0.3",
    "aws-sdk": "^2.192.0",
    "bcrypt": "^1.0.3",
    "bluebird": "^3.5.1",
    "body-parser": "^1.18.2",
    "busboy": "^0.2.14",
    "connect-busboy": "0.0.2",
    "cors": "^2.8.4",
    "cross-env": "5.1.2",
    "dotenv": "^5.0.0",
    "draft-js": "^0.10.5",
    "express": "^4.16.2",
    "express-fileupload": "^0.4.0",
    "file-system": "^2.2.2",
    "fs": "0.0.1-security",
    "history": "^4.7.2",
    "http": "0.0.0",
    "jsonwebtoken": "^8.1.0",
    "material-ui": "^1.0.0-beta.29",
    "mongoose": "^5.0.0",
    "nodemon": "^1.14.11",
    "passport": "^0.4.0",
    "passport-local": "^1.0.0",
    "pdfjs-dist": "^2.0.391",
    "pdfreader": "^0.2.4",
    "promise": "^8.0.1",
    "pryjs": "^1.0.3",
    "react": "16.2.0",
    "react-addons-update": "^15.6.2",
    "react-autosuggest": "^9.3.4",
    "react-bootstrap": "^0.32.0",
    "react-bootstrap-modal": "^4.0.0",
    "react-dom": "16.2.0",
    "react-dropzone": "^4.2.8",
    "react-file-viewer": "^0.3.49",
    "react-hot-loader": "3.1.3",
    "react-modal-dialog": "^4.0.7",
    "react-onscroll": "^1.0.4",
    "react-pdf-js": "^3.0.7",
    "react-quill": "^1.2.6",
    "react-router-dom": "^4.2.2",
    "react-scroll": "^1.7.9",
    "react-tap-event-plugin": "^3.0.2",
    "s3fs": "git+https://github.com/RiptideElements/s3fs.git",
    "socket.io": "^2.1.0",
    "socket.io-client": "^2.1.0",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.1.2",
    "babel-jest": "22.0.6",
    "babel-loader": "7.1.2",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react": "6.24.1",
    "babel-preset-react-boilerplate": "1.1.1",
    "babel-preset-react-hmre": "1.1.1",
    "babel-preset-stage-2": "6.24.1",
    "copy-webpack-plugin": "4.3.0",
    "css-loader": "0.28.8",
    "enzyme": "3.3.0",
    "enzyme-adapter-react-16": "1.1.1",
    "eslint": "4.13.0",
    "eslint-config-airbnb": "16.1.0",
    "eslint-loader": "1.9.0",
    "eslint-plugin-import": "2.8.0",
    "eslint-plugin-jsx-a11y": "6.0.3",
    "eslint-plugin-react": "7.5.1",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.6",
    "html-webpack-plugin": "2.30.1",
    "husky": "0.15.0-beta.2",
    "jest": "22.0.5",
    "node-sass": "4.7.2",
    "open-browser-webpack-plugin": "0.0.5",
    "react-addons-test-utils": "15.6.2",
    "react-test-renderer": "16.2.0",
    "rimraf": "2.6.2",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.1",
    "url-loader": "0.6.2",
    "webpack": "3.10.0",
    "webpack-dev-server": "2.9.6"
  }
}
